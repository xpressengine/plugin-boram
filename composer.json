{
    "name": "xpressengine-plugin/boram",
    "description": "회사 소개형 사이트를 제작시에 어울리는 테마와 위젯을 제공하고 있습니다.",
    "keywords": [
        "xpressengine",
        "plugin"
    ],
    "license": "LGPL-3.0-or-later",
    "version": "1.0.3",
    "type": "xpressengine-plugin",
    "support": {
        "email": "developer@xpressengine.com"
    },
    "authors": [
        {
            "name": "XE Developers",
            "email": "developer@xpressengine.com",
            "homepage": "https://store.xehub.io/plugins/boram",
            "role": "Developer"
        }
    ],
    "extra": {
        "xpressengine": {
            "title": "Boram Theme",
            "component": {
                "theme/boram@theme": {
                    "class": "Xpressengine\\Plugins\\Boram\\Theme\\Theme",
                    "name": "Boram Theme",
                    "description": "The Theme supported by Boram plugin."
                }
            }
        }
    },
    "require": {
        "xpressengine/xpressengine": "~3.0.2"
    },
    "autoload": {
        "psr-4": {
            "Xpressengine\\Plugins\\Boram\\": "src/"
        },
        "classmap": [
            "theme/Theme.php"
        ]
    }
}
